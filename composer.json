    "type": "silverstripe-module",
    "keywords": ["silverstripe", "authentication", "saml", "RealMe", "Real Me", "iGovt"],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Matt Peel",
            "email": "matt@silverstripe.com"
        },
        {
            "name": "Simon Gow",
            "email": "simon@silverstripe.com"
        }
    ],
    "require": {
        "composer/installers": "*",
        "silverstripe/framework": "~3.1",
        "madmatt/simplesamlphp": "dev-ss-master",
        "simplesamlphp/saml2": "v0.8.1 as dev-master"
    },
    "require-dev": {
        "hafriedlander/silverstripe-phockito": "*"
    },
    "support": {
